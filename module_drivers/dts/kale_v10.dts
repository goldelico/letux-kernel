&dpu {
	status = "okay";
	ingenic,disable-rdma-fb = <1>;
	//[>Defines the init state of composer fb export infomations.<]
	ingenic,layer-exported = <1 1 0 0>;
	ingenic,layer-frames   = <2 2 2 2>;
	ingenic,layer-framesize = <800 480>, <800 480>, <320 240>, <320 240>;   //[>Max framesize for each layer.<]
	layer,color_mode	= <0 0 0 0>;					//[>src fmt,<]
	layer,src-size       	= <800 480>, <800 480>, <320 240>, <240 200>;	//[>Layer src size should smaller than framesize<]
	layer,target-size	= <800 480>, <800 240>, <160 240>, <240 200>;	//[>Target Size should smaller than src_size.<]
	layer,target-pos	= <0 0>, <0 240>, <340 480>, <100 360>;	//[>target pos , the start point of the target panel.<]
	layer,enable		= <1 1 1 1>;					//[>layer enabled or disabled.<]
	ingenic,logo-pan-layer	= <0>;						//[>on which layer should init logo pan on.<]
	port {
		dpu_out_ep: endpoint {
#if LCD_PANEL_MA0060
			remote-endpoint = <&panel_ma0060_ep>;
#else
			remote-endpoint = <&panel_kd050wvfpa029_ep>;
#endif
	    };
	};
};

