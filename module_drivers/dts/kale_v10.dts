	 */
	bt_power {
		compatible = "ingenic,bt_power";
		ingenic,reg-on-gpio = <&gpd 20 GPIO_ACTIVE_LOW INGENIC_GPIO_NOBIAS>;
		ingenic,wake-gpio = <&gpd 22 GPIO_ACTIVE_LOW INGENIC_GPIO_NOBIAS>;
	};

	sound {
		compatible = "ingenic,x2000-sound";
		ingenic,model = "kale_v10";
		ingenic,spken-gpio = <&gpd 18 GPIO_ACTIVE_HIGH INGENIC_GPIO_NOBIAS>;
	};

	display-dbi {
		compatible = "simple-bus";
		#interrupt-cells = <1>;
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <>;

#if LCD_PANEL_MA0060
		panel_ma0060 {
			compatible = "ingenic,ma0060";
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&smart_lcd_pb_te>;
			ingenic,vdd-en-gpio = <&gpc 4 GPIO_ACTIVE_LOW INGENIC_GPIO_NOBIAS>;
