#include "x1000.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/clock/ingenic,sysost.h>
#include <dt-bindings/interrupt-controller/irq.h>

/ {
	compatible = "sudomaker,x1000-nano-evb", "ingenic,x1000e";
	model = "SudoMaker X1000 Nano EVB (NAND flash)";

	aliases {
		serial2 = &uart2;
	};

	chosen {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		stdout-path = "serial2:115200n8";
		//bootargs = "console=ttyS2,115200n8 mem=64M@0x0 loglevel=7 no_hash_pointers no_console_suspend devtmpfs.mount=1 ingenic_pm_clk=1 ip=off init=/linuxrc ubi.mtd=2 ubi.block=0,0 root=/dev/ubiblock0_0 rootwait";
		// bootargs = "console=ttyS2,115200n8 mem=64M@0x0 loglevel=7 no_hash_pointers no_console_suspend devtmpfs.mount=1 ingenic_pm_clk=1 ip=off init=/bin/ash ubi.mtd=2 ubi.block=0,0 root=/dev/ubiblock0_0 rootwait";

		framebuffer0: framebuffer@3F00000 {
			compatible = "simple-framebuffer";
			reg = <0x03F00000 (240 * 320 * 4)>;
			width = <240>;
			height = <320>;
			stride = <(240 * 4)>;
			format = "a8r8g8b8";
			status = "okay";
		};

		// framebuffer0: framebuffer@3F00000 {
		// 	compatible = "simple-framebuffer";
		// 	reg = <0x03F00000 (320 * 240 * 4)>;
		// 	width = <320>;
		// 	height = <240>;
		// 	stride = <(320 * 4)>;
		// 	format = "a8r8g8b8";
		// 	status = "okay";
		// };
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x04000000>;
	};

	gpio_keys {
		compatible = "gpio-keys";

		btn_lf {
			wakeup-source;
			label = "BOOT0";
			linux,code = <KEY_F1>;
			gpios = <&gpb 28 GPIO_ACTIVE_LOW>;
		};

		btn_rf {
			wakeup-source;
			label = "BOOT1";
			linux,code = <KEY_F2>;
			gpios = <&gpb 29 GPIO_ACTIVE_LOW>;
		};

		btn_wkup {
			wakeup-source;
			label = "WKUP";
			linux,code = <KEY_F3>;
			gpios = <&gpb 31 GPIO_ACTIVE_LOW>;
		};
	};

	sound_internal_codec {
		status = "okay";

		compatible = "simple-audio-card";

		simple-audio-card,name = "x1000-aic";
		simple-audio-card,bitclock-master = <&dai_codec>;
		simple-audio-card,frame-master = <&dai_codec>;
		simple-audio-card,format = "i2s";
		simple-audio-card,aux-devs = <&hpamp>;

		dai_cpu: simple-audio-card,cpu {
			sound-dai = <&aic>;
			system-clock-frequency = <24000000>;
			system-clock-direction-out;
		};

		dai_codec: simple-audio-card,codec {
			sound-dai = <&codec>;
			system-clock-frequency = <24000000>;
		};
	};

	sound_i2s_output_only {
		status = "disabled";

		compatible = "simple-audio-card";

		simple-audio-card,name = "x1000-aic";

		simple-audio-card,mclk-fs = <256>;
		simple-audio-card,bitclock-master = <&dai_codec1>;
		simple-audio-card,frame-master = <&dai_codec1>;
		simple-audio-card,format = "i2s";

		dai_cpu1: simple-audio-card,cpu {
			sound-dai = <&aic>;
			system-clock-direction-out;
		};

		dai_codec1: simple-audio-card,codec {
			sound-dai = <&i2s_raw_out>;
		};
	};

	i2s_raw_out: i2s-generic {
		#address-cells = <0>;
		#size-cells = <0>;
		#sound-dai-cells = <0>;
		compatible = "linux,spdif-dit";
		status = "okay";
	};

	clk_8m: clock {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <8000000>;
		clock-accuracy = <100>;
	};
};

&exclk {
	clock-frequency = <24000000>;
};

&cgu {
	assigned-clocks = <&cgu X1000_CLK_SSIMUX>, <&cgu X1000_CLK_RTC>;
	assigned-clock-parents = <&cgu X1000_CLK_SSIPLL_DIV2>, <&cgu X1000_CLK_RTCLK>;
};

&ost {
	/* 1500 kHz for the system timer and clocksource */
	assigned-clocks = <&ost OST_CLK_EVENT_TIMER>, <&ost OST_CLK_GLOBAL_TIMER>;
	assigned-clock-rates = <1500000>, <1500000>;
};

&aic {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pins_i2s>;

	use-internal-codec;
};

&codec {
	status = "okay";
};

&dmic {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pins_dmic>;
};

