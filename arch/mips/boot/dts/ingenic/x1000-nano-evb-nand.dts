&ssi {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pins_spi0>;

	num-cs = <1>;
	cs-gpios = <0>;

	spidev: spidev@0 {
		reg = <0>;
		compatible = "sudomaker,spidev-zerocopy";

		spi-max-frequency = <50000000>;
	};

};

&uart0 {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pins_uart0>;

	uart-has-rtscts;

	bluetooth {
		compatible = "brcm,bcm43438-bt";
		max-speed = <1500000>;
		brcm,bt-pcm-int-params = [01 02 00 01 01];
		reset-gpios = <&gpc 18 GPIO_ACTIVE_LOW>;
		device-wakeup-gpios = <&gpc 19 GPIO_ACTIVE_HIGH>;
		interrupt-parent = <&gpc>;
		interrupts = <20 IRQ_TYPE_EDGE_FALLING>;

		pinctrl-names = "default";
		pinctrl-0 = <&pins_rtc32k>;
	};
};

&uart2 {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pins_uart2>;
};

&i2c0 {
	status = "okay";

	clock-frequency = <100000>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_i2c0>;

	// ts3a227e: ts3a227e@3b {
	// 		compatible = "ti,ts3a227e";
	// 		reg = <0x3b>;
	// 		interrupt-parent = <&gpc>;
	// 		interrupts = <25 IRQ_TYPE_LEVEL_LOW>;
	// };

	// codec_nau88c22: nau88c22@1a {
	// 	compatible = "nuvoton,nau8822";
	// 	reg = <0x1a>;

	// 	#sound-dai-cells = <0>;
	// };
};

&i2c1 {
	status = "okay";

	clock-frequency = <20000>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_i2c1>;

	hpamp: tpa6130a2@60 {
		compatible = "ti,tpa6130a2";
		reg = <0x60>;
		power-gpio = <&gpc 23 GPIO_ACTIVE_HIGH>;
	};
};

&sfc {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_sfc>;

	flash: spinand@0 {
		compatible = "spi-nand";
		spi-max-frequency = <133000000>;
		reg = <0>;

		spi-rx-bus-width = <4>;
		spi-tx-bus-width = <4>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot@0 {
				reg = <0x0 0x100000>;
			};

			kernel@100000 {
				reg = <0x100000 0x600000>;
			};

			rootfs@700000 {
				reg = <0x700000 0xFB00000>;
			};
		};
	};

};

&msc0 {
	status = "okay";

	bus-width = <4>;
	max-frequency = <60000000>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_msc0>;

	non-removable;
};

&msc1 {
		status = "okay";

		#address-cells = <1>;
		#size-cells = <0>;
		bus-width = <4>;
		max-frequency = <50000000>;

		pinctrl-names = "default";
		pinctrl-0 = <&pins_msc1>;

		non-removable;
		cap-power-off-card;
		keep-power-in-suspend;
		wakeup-source;

		//mmc-pwrseq = <&wlan_pwrseq>;

		ap6212a: wifi@1 {
				compatible = "brcm,bcm4329-fmac";
				reg = <1>;

				interrupt-parent = <&gpc>;
				interrupts = <16 IRQ_TYPE_EDGE_FALLING>;
				interrupt-names = "host-wake";
				wakeup-source;

				reset-gpios = <&gpc 17 GPIO_ACTIVE_LOW>;

				brcm,drive-strength = <10>;
		};
};

&mac {
	status = "okay";

	phy-mode = "rmii";
	phy-handle = <&dp83825i>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_mac>;
};

&mdio {
	status = "okay";

	dp83825i: ethernet-phy@0 {
		reg = <0>;

		interrupt-parent = <&gpc>;
		interrupts = <22 IRQ_TYPE_LEVEL_LOW>;
		wakeup-source;

		reset-names = "phy";
		reset-gpios = <&gpb 25 GPIO_ACTIVE_LOW>;
		reset-assert-us = <10000>;
		reset-deassert-us = <30000>;
	};
};

&otg_phy {
	status = "okay";
};

&otg {
	status = "okay";
};

&pinctrl {
	pins_rtc32k: rtc32k {
		function = "rtc32k";
		groups = "rtc32k";
	};

	pins_sfc: sfc {
		function = "sfc";
		groups = "sfc-data", "sfc-clk", "sfc-ce";
	};

	pins_slcd: slcd {
		function = "lcd";
		groups = "lcd-8bit";
	};

	pins_emc: emc {
		function = "lcd";
		groups = "lcd-8bit";
	};

	pins_spi0: spi0 {
		function = "ssi";
		groups = "ssi-dt-d", "ssi-dr-d", "ssi-clk-d", "ssi-ce0-d";
	};

	pins_uart0: uart0 {
		function = "uart0";
		groups = "uart0-data", "uart0-hwflow";
	};

	pins_uart2: uart2 {
		function = "uart2";
		groups = "uart2-data-d";
	};

	pins_i2c0: i2c0 {
		function = "i2c0";
		groups = "i2c0-data";
		bias-disable;
	};

	pins_i2c1: i2c1 {
		function = "i2c1";
		groups = "i2c1-data-c";
		bias-pull-up;
	};

	pins_i2s: i2s {
		function = "i2s";
		groups = "i2s-data-tx", "i2s-data-rx", "i2s-clk-txrx", "i2s-sysclk";
	};

	pins_dmic: dmic {
		function = "dmic";
		groups = "dmic-if0", "dmic-if1";
	};

	pins_msc0: msc0 {
		function = "mmc0";
		groups = "mmc0-1bit", "mmc0-4bit";
	};

	pins_msc1: msc1 {
		function = "mmc1";
		groups = "mmc1-1bit", "mmc1-4bit";
	};

	pins_mac: mac {
		function = "mac";
		groups = "mac";
		bias-disable;
	};
};
