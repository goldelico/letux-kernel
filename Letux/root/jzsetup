function offsettoaddr {
	case "$1" in
		HDMI_* ) BASE=$HDMI; FACTOR=4 ;;
		JZ_REG* ) BASE=$LCDC; FACTOR=1 ;;
	esac
	printf "0x%x" $((0x$BASE + $FACTOR * $2))
}

function peek {
	VAL=$(devmem2 $1 w | fgrep 'Value at address' | cut -d ' ' -f 6)
	printf %08x $[$VAL]
}

(lcdcregisters; hdmiregisters) | while read DEFINE REGISTER OFFSET
do
	case "$DEFINE" in
	'#define' )
		ADDR=$(offsettoaddr $REGISTER $OFFSET)
		NEWVAL=$(value "$ADDR")
		[ "$NEWVAL" ] || continue
		VAL=$(peek $ADDR)
		[ "$NEWVAL" != "$VAL" ] || continue
		echo "$ADDR $VAL => $NEWVAL $REGISTER"
		devmem2 $ADDR w 0x$NEWVAL
		;;
	esac
done

cat /sys/kernel/debug/regmap/$LCDC.lcd*/registers
