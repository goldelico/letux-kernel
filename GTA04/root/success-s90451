# non-reload MTP
# CMD1
echo FF 00 >dcs
echo FB 01 >dcs
# CMD2 Page0
echo FF 01 >dcs
echo FB 01 >dcs
# CMD2 Page1
echo FF 02 >dcs
echo FB 01 >dcs
# CMD2 Page2
echo FF 03 >dcs
echo FB 01 >dcs
# CMD2 Page3
echo FF 04 >dcs
echo FB 01 >dcs
# CMD2 Page4
echo FF 05 >dcs
echo FB 01 >dcs

fi

echo ff00 >dcs
echo fb01 >dcs
echo ba03 >dcs	# switch to 4 lanes
echo ba r >dcs	# check
echo c203 >dcs
echo c2 r >dcs	# check

# DCS sleep out
echo 11 >dcs
sleep 0.100

if false
then
echo 51ff >dcs
echo 532c >dcs
echo 5500 >dcs
fi

# enable stream and turn display on
echo 29 >dcs
sleep 0.020
echo stream >dcs

# randomly fill framebuffer
while true
do
cat /dev/urandom > /dev/fb0 2>/dev/null
done